<div class="overview_title">
  &nbsp; <%= name.upcase %>
</div>

<div>
  <% if check_partial_exists(path + "/_overview") %>
    <%= render partial: path + "/overview" %>
  <% end %>
</div>

<hr style="visibility: hidden; line-height: 10px;">
<div style="margin-left: 10px; font-size: 12px;">
  <a href="<%= path %>" class="tiny button" target="_blank">
    Proceed to <%= name %>
  </a>
</div>