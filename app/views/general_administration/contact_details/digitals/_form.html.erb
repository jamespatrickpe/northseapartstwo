<%= f.input :url,
              :as => :url,
              :label => "URL",
              label_html: { class: 'form_label' },
              :input_html =>
                      {
                              :value => @selected_digital[:url],
                              :'data-parsley-trigger' => 'keyup',
                              :'data-parsley-maxlength' => 256
                      }
%>

<%= generic_form_remark_field(f, @selected_digital[:remark]) %>
<%= polymorphic_selector(@selected_digital,'digitable',f) %>