<section class='small-10 columns'>
  <%= preheader_generic_table('Contact Details for any Digital Medium', 'Digitals') %>
      <% begin %>
      <table class='generic_table' id='digital_table'>
        <%= generate_theadlinks('ID', 'URL', 'remark','Created At', 'Updated At') %>
        <% if @digitals.empty? == true %>
            <%= no_records_found %>
        <% else %>
            <tbody>
            <% @digitals.each do |digital| %>
                <tr>
                  <td><%= digital[:id].upcase %></td>
                  <td><%= digital[:url] %> </td>
                  <td><%= digital[:remark] %> </td>
                  <td><%= digital[:created_at] %> </td>
                  <td><%= digital[:updated_at] %> </td>
                  <%= generic_table_actions(digital[:id]) %>
                </tr>
            <% end %>
            </tbody>
        <% end %>
      </table>
  <% rescue => ex %>
      <%= generic_table_error(ex) %>
  <% end %>
  <%= generic_table_footer(@digitals) %>
</section>